# Read the dataset
file_path <- getwd()
paste(file_path)
df <- read.csv(paste0(file_path, "share-electricity-renewables.csv"),header=TRUE)
df[1:15,] %>%
kable(format = "html", col.names = colnames(df)) %>%
column_spec(c(2:21), width_min = "0.8in")  %>%
kable_styling() %>%
kableExtra::scroll_box(width = "100%", height = "300px")
